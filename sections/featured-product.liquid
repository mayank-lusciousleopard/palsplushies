<script>
  document.addEventListener('DOMContentLoaded', function() {
    const gallery = document.getElementById('Gallery-{{ section.id }}');
    if (gallery) {
      const mainImg = document.getElementById('MainImage-ll');
      const thumbs = gallery.querySelectorAll('.custom-thumbnail');

      thumbs.forEach(thumb => {
        thumb.addEventListener('click', function() {
          const newSrc = this.getAttribute('data-target-src');
            mainImg.src = newSrc;
              mainImg.srcset = newSrc;
        });
      });
    }
  });
</script>

